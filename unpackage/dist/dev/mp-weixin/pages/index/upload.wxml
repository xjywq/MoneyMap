<view class="uni-max-wrap"><view class="uni-header-text"><text>{{income_text}}</text><button class="change_income_button" type="default" data-event-opts="{{[['tap',[['change_income',['$event']]]]]}}" bindtap="__e">切换</button></view><view class="uni-padding-wrap uni-common-mt"><form data-ref="submit_form" data-event-opts="{{[['submit',[['formSubmit',['$event']]]],['reset',[['formReset',['$event']]]]]}}" class="vue-ref" bindsubmit="__e" bindreset="__e"><view class="uni-form-item uni-column"><view class="title">价格</view><input class="uni-input" name="price" focus="{{true}}" placeholder="请输入价格" data-event-opts="{{[['input',[['__set_model',['','price','$event',[]]]]]]}}" value="{{price}}" bindinput="__e"/></view><view class="uni-form-item uni-column"><view class="title">标签</view><radio-group name="tag"><block wx:for="{{radioItems}}" wx:for-item="item" wx:for-index="index"><view class="uni-list-cell"><view><radio id="item.value" value="{{item.value}}" checked="{{item.checked}}"></radio></view><label class="label-2-text"><text>{{item.value}}</text></label></view></block></radio-group></view><view class="uni-form-item uni-column"><view class="title">必要</view><view><switch name="isimportant" checked="true"></switch></view></view><view class="uni-form-item uni-column"><view class="title">备注</view><input class="uni-input" name="comment" placeholder="备注" data-event-opts="{{[['input',[['__set_model',['','comment','$event',[]]]]]]}}" value="{{comment}}" bindinput="__e"/></view><view class="uni-btn-v"><button form-type="submit">确认</button><button type="default" data-event-opts="{{[['tap',[['formReset',['$event']]]]]}}" bindtap="__e">返回</button></view></form></view></view>