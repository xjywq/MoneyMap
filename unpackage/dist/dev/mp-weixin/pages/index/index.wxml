<view><view class="uni-padding-wrap uni-common-mt"><view class="uni-btn-v"><button type="primary" data-event-opts="{{[['tap',[['openDB',['$event']]]]]}}" bindtap="__e">打开数据库Mymoney.db</button></view><view class="uni-btn-v"><button type="primary" data-event-opts="{{[['tap',[['selectSQL',['$event']]]]]}}" bindtap="__e">查询表database的数据</button></view><view class="uni-btn-v"><button type="primary" data-event-opts="{{[['tap',[['droptable',['$event']]]]]}}" bindtap="__e">删除表database</button></view><view class="uni-btn-v"><button type="primary" data-event-opts="{{[['tap',[['closeDB',['$event']]]]]}}" bindtap="__e">关闭数据库Mymoney.db</button></view><button data-event-opts="{{[['tap',[['navigateTo',['$event']]]]]}}" class="jump" bindtap="__e">上传</button><button data-event-opts="{{[['tap',[['updateClick',['$event']]]]]}}" class="jump" bindtap="__e">刷新</button><block wx:for="{{sql_data}}" wx:for-item="item" wx:for-index="index"><view class="uni-list-cell"><view><label class="_span">{{item.price}}<view class="_br"></view>{{item.tags}}</label><view class="_br"></view><label class="_span">{{item.comment}}<view class="_br"></view>{{item.income}}</label></view></view></block></view></view>